{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "grpc": "Information"
    }
  },
  "ConnectionStrings": {
    "ProductsContext": "Server=product.grpc.db;Port=5438;Database=products;User Id=postgres;Password=postgres;"
  },
  "Kestrel": {
    "EndpointDefaults": {
      "Protocols": "Http2",
      "SslProtocols": [ "Tls12", "Tls13" ],
      "ClientCertificateMode": "AllowCertificate"
    },
    "EndPoints": {
      "Https": {
        "Url": "https://productgrpc.internal.local:443",
        "Port": 443,
        "Certificate": {
          "Path": "Infrastructure/productgrpc.internal.local.pfx",
          "Password": "productgrpc.internal.local",
          "AllowInvalid": false
        }
      }
    }
  },
  "AllowedHosts": "*"
}
